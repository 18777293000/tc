.time {
  display: flex;
  padding: 35rpx 0;
  margin: 0 20rpx;
  border-bottom: 1px solid #e5e5e5;
  justify-content: space-between;
}

.tip-content-dialog {
  position: fixed;
  display: flex;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(128, 126, 126, 0.555);
  z-index: 99999;
}

.tip-content-dialog .tip-dialog-view {
  width: 80%;
  margin: auto;
  border-radius: 25rpx;
  vertical-align: middle;
  animation: tanchu 400ms ease-in;
  /* overflow: hidden; */
  padding: 20rpx;
}

.tip-content-dialog .btn {
  background: #f2f7fa;
}

@keyframes tanchu {
  from {
    transform: scale(0, 0);
    -webkit-transform: scale(0, 0);
  }

  to {
    transform: scale(1, 1);
    -webkit-transform: scale(1, 1);
  }
}

.tip-content-dialog .dialogClose {
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}

.guan {
  position: absolute;
  bottom: 20px;
  left: 44%;
}

.guan image {
  width: 100rpx;
  height: 100rpx;
}

.quan {
  margin: 4% 0;
  background: #fff;
  border-radius: 10px;
  display: flex;
  height: 140rpx;
  align-items: center;
}