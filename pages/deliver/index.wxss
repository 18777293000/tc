  justify-content:center;
}

.trs view:nth-child(2){
  width:18%;
  border-bottom:0;
  border-left:0;
  border-right:0;
   display: flex;
  flex-wrap:wrap;
  justify-content:center;
  text-align:center;

}
.trs view:nth-child(3){
width:18%;
border-bottom:0;
 display: flex;
  flex-wrap:wrap;
  justify-content:center;
