/* pages/user/address/add/index.wxss */
.pages{
  width: 100vw;
  height: 100vh;
  padding: 20rpx;
  box-sizing: border-box;
  background: #f2f2f2
}
.message_box{
  border-radius: 20rpx;
  padding: 24rpx;
  box-sizing: border-box;
  border: 1px solid #d2d2d2;
  background: white
}
.list_line{
  display: flex;
  padding: 20rpx;
  font-size: 28rpx;
  color: #666;
  align-items: center;
  border-bottom: 1px solid #d2d2d2;
  
}
.list_line:last-child{
  border: none
}
.save{
  width: 100%;
  box-sizing: border-box;
  background: #e7d0ae;
  color: #fff;
  text-align: center;
  border-radius: 20rpx;
  margin-top: 380rpx;
  letter-spacing: 10rpx;
  font-size: 32rpx;
}

button::after{
  border: none
}