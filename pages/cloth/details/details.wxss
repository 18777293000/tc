.tip-content-dialog {
  position: fixed;
  display: flex;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(128, 126, 126, 0.555);
  z-index: 99999;
}

.tip-content-dialog .tip-dialog-view {
  width: 80%;
  margin: auto;
  border-radius: 25rpx;
  vertical-align: middle;
  animation: tanchu 400ms ease-in;
  /* overflow: hidden; */
  padding: 20rpx;
}

.tip-content-dialog .btn {
  background: #f2f7fa;
}

@keyframes tanchu {
  from {
    transform: scale(0, 0);
    -webkit-transform: scale(0, 0);
  }

  to {
    transform: scale(1, 1);
    -webkit-transform: scale(1, 1);
  }
}

.tip-content-dialog .dialogClose {
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}

.guan {
  position: absolute;
  bottom: 0;
  left: 44%;
}

.guan image {
  width: 100rpx;
  height: 100rpx;
}

.quan {
  position: relative;
  display: flex;
  width: 100%;
  background-image: url('https://pl.shipucm.cn/image/yh.png');
  background-size: contain;
  height: 165rpx;
  background-repeat: no-repeat;
  align-items: center;
}