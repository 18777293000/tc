/*主容器*/  
.stepper {  
    /* width: 80px;   */
    height: 52rpx;  
    /*给主容器设一个边框*/  
    /* border: 1px solid #ccc;   */
    border-radius: 6rpx;  
    margin:0 auto;  
}  
/*加号和减号*/  
.stepper text {  
    width: 48rpx;  
    line-height: 48rpx;  
    text-align: center;  
    float: left;
    border-radius: 50%;
    border:1px solid #caae7f;
    font-weight: 600;
}  
  
/*数值*/  
.stepper input {  
    width: 80rpx;  
